{
	"name": "pendingsystem",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "node --max-old-space-size=4096 node_modules/next/dist/bin/next dev",
		"prepare": "husky install",
		"build": "next build",
		"lint": "biome check .",
		"start": "next start",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:ui": "vitest --ui",
		"e2e": "playwright test",
		"e2e:ui": "playwright test --ui",
		"e2e:debug": "playwright test --debug",
		"e2e:headed": "playwright test --headed",
		"e2e:report": "playwright show-report",
		"docs": "echo 'API documentation is maintained in ./docs folder'",
		"docs:serve": "cd docs && python3 -m http.server 8080 || python -m http.server 8080",
		"docs:validate": "node scripts/validate-docs.js",
		"docs:extract": "node scripts/extract-jsdoc.js",
		"testsprite": "echo \"To run TestSprite tests, use an MCP-compatible IDE and enter: Help me test this project with TestSprite.\"",
		"commit": "pwsh scripts/quick-commit.ps1"
	},
	"dependencies": {
		"@ark-ui/react": "^5.30.0",
		"@radix-ui/react-checkbox": "^1.1.3",

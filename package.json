		"@testing-library/react": "^16.3.1",
		"@testing-library/user-event": "^14.6.1",
		"@testsprite/testsprite-mcp": "^0.0.18",
		"@types/node": "^20",
		"@types/react": "^19.0.0",
		"@types/react-dom": "^19.0.0",
		"@vitejs/plugin-react": "^5.1.2",
		"autoprefixer": "^10.4.20",
		"husky": "^9.1.7",
		"jsdom": "^27.3.0",
		"postcss": "^8.5.1",
		"tailwindcss": "^3.4.17",
